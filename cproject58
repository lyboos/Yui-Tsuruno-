void reverse(char arr[],int a)
{
	int len = strlen(arr);
	int i, j;
	for ( i = 0; i <a; i++)
	{
		char tmp = *arr;
			j = 0;
		for ( j = 0; j < len-1; j++)
		{
			*(arr + j) = *(arr + j + 1);
		}
		*(arr + len-1) = tmp;
	}
}
int main()
{
	char arr[100] = { 0 };
	scanf("%s", arr);
	int a;
	scanf("%d",&a);
	reverse(arr,a);
	printf("%s", arr);
	return 0;
}
