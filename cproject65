#define  _CRT_SECURE_NO_WARNINGS 
#include<stdio.h>
//#include<string.h>
//#include<assert.h>
int strlen(char* a)
{
	int i = 0;
	while (*(a+i)!='\0')
	{
		i++;
	}
	return i;
}
int fun(char* a)
{
	int i = 0,flag=0,num=0,check=0;
	for ( i = 0; i <strlen(a); i++)
	{
		if (a[i] == ' ' && flag == 0)
			continue;
		if (a[i] == '-'&&flag==0)
		{
			check = 1;
			flag = 1;
			continue;
		}
		if (*(a+i)>='0'&&*(a+i)<='9')
		{
			num=*(a+i)-48+num*10;
			flag = 1;
		}
		else if(flag)
		{
			*(a + i) = '\0';
			break;
		}
	}
	if (check)
		return -num;
	else if (flag)
		return num;
	return 0;
}

int main()
{
	char a[40];
	scanf("%s", a);
	printf("%d", fun(a));
	return 0;
}
